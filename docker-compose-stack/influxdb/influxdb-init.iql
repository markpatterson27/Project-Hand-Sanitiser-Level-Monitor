CREATE DATABASE "HomeMonitor"
CREATE RETENTION POLICY "2_weeks" ON "HomeMonitor" DURATION 2w REPLICATION 1 DEFAULT
CREATE RETENTION POLICY "4_years" ON "HomeMonitor" DURATION 208w REPLICATION 1
CREATE CONTINUOUS QUERY "cq_1h" ON "HomeMonitor" BEGIN SELECT mean(*) INTO "4_years"."downsampled-readings" FROM "sensor-reading
s" GROUP BY time(60m), * END
